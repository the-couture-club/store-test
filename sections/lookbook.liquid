<!-- The following plugins are only available for Club GSAP members.--> 
<script src="{{ 'ScrollSmoother.min.js' | asset_url }}"></script>
<script src="{{ 'SplitText.min.js' | asset_url }}"></script>

<script>


document.addEventListener("DOMContentLoaded", (event) => {

  var containers = document.querySelectorAll('.container')

  containers.forEach((container) => {
    container.style.height = window.innerHeight + 'px'
  });

  gsap.registerPlugin(ScrollTrigger,TextPlugin,ScrollSmoother,SplitText);


  var videos = document.querySelectorAll('#full-video');

  videos.forEach((video) => {

    var vid = video.querySelector('video');
    var playPause = video.querySelector('.video-play-pause');

    playPause.addEventListener('click', () => {
     if(vid.paused) {
      playPause.setAttribute('aria-play', 'true')
      vid.play();
     } else {
      vid.pause();
      playPause.setAttribute('aria-play', 'false')
     }
    });

    var muteUnmute = video.querySelector('.video-mute-unmute');

    muteUnmute.addEventListener('click', () => {
     if(vid.muted) {
      muteUnmute.setAttribute('aria-muted', 'false')
      vid.muted = false;
     } else {
      muteUnmute.setAttribute('aria-muted', 'true')
      vid.muted = true;
     }
    });



  });

  // THE RESORT

  const cursor = document.querySelector('.resort-cursor');


  const positionElement = (e)=> {
    const mouseY = e.clientY;
    const mouseX = e.clientX;

    cursor.style.transform = `translate3d(${mouseX}px, ${mouseY}px, 0) translate(-50%, -50%)`;

  }

  document.getElementById('the-resort').addEventListener('mousemove', positionElement);

  var resortVideoContainer = document.querySelector('.resort-video#full-video');
  var docElement = document.querySelector('html');
  var run

  document.querySelector('#the-resort .resort-cursor').addEventListener('click', function () {
    gsap.to(resortVideoContainer, {
          autoAlpha: 1,
          duration: 0.5
        }),
        gsap.to('.js-section__header', {
          autoAlpha: 0,
          duration: 0.5
        })
        var run = setInterval(function () { videoDuration(resortVideoContainer); }, 10);
        docElement.classList.add("no-scroll");
        resortVideoContainer.querySelector('video').play();
  });

  document.querySelector('#the-resort .mobile-video-toggle').addEventListener('click', function () {
    gsap.to(resortVideoContainer, {
          autoAlpha: 1,
          duration: 0.5
        }),
        gsap.to('.js-section__header', {
          autoAlpha: 0,
          duration: 0.5
        })
        var run = setInterval(function () { videoDuration(resortVideoContainer); }, 10);
        docElement.classList.add("no-scroll");
        resortVideoContainer.querySelector('video').play();
  });

  document.querySelector('.resort-video .video-close.desk').addEventListener('click', function () {
    gsap.to(resortVideoContainer, {
          autoAlpha: 0,
          duration: 0.5,
          onComplete: function() {
            document.querySelector('#the-resort .video-play-pause').setAttribute('aria-play', 'true');
          }
        }),
        gsap.to('.js-section__header', {
          autoAlpha: 1,
          duration: 0.5
        })
      
        clearTimeout(run);
        docElement.classList.remove("no-scroll");
        resortVideoContainer.querySelector('video').pause();
  });

  document.querySelector('.resort-video .video-close.mob').addEventListener('click', function () {
    gsap.to(resortVideoContainer, {
          autoAlpha: 0,
          duration: 0.5,
          onComplete: function() {
            document.querySelector('#the-resort .video-play-pause').setAttribute('aria-play', 'true');
          }
        }),
        gsap.to('.js-section__header', {
          autoAlpha: 1,
          duration: 0.5
        })
      
        clearTimeout(run);
        docElement.classList.remove("no-scroll");
        resortVideoContainer.querySelector('video').pause();
  });

  document.querySelector('.resort-video video').addEventListener("fullscreenchange", (event) => {
      if (document.fullscreenElement) {
      alert(
        `Element: ${document.fullscreenElement.id} entered fullscreen mode.`,
      );
    } else {
      alert("Leaving fullscreen mode.");
    }
  });

  var mySplitText = new SplitText("#the-resort .title", {type:"chars, words"}),
    tl = new TimelineLite(),
    numChars = mySplitText.chars.length;

  for(var i = 0; i < numChars; i++){
    tl.from(mySplitText.chars[i], 2, {opacity:0}, Math.random() * 1);
  }

  var mySplitText = new SplitText("#the-resort .content-container_copy span", {type:"chars, words"}),
  tl = new TimelineLite(),
  numChars = mySplitText.chars.length;

for(var i = 0; i < numChars; i++){
  tl.from(mySplitText.chars[i], 2, {opacity:0, delay: 3}, Math.random() * 1);
}


  gsap.to(".mobile-video-toggle", {
    opacity: 1,
    ease: "none",
    delay: 5,
    duration: 1
  });

  gsap.to(".content-by", {
    opacity: 1,
    ease: "none",
    delay: 2,
    duration: 1
  });

  gsap.to("#the-resort .container-background", {
    opacity: 0.8,
    ease: "none",
    duration: 3
  });


  // palette

  const palette = document.querySelectorAll('.palette-content [data-colour]');
  
  palette.forEach(colour => {
    colour.addEventListener('click', function () {
      var clickBackground = document.querySelector("img[data-colour='" + colour.getAttribute('data-colour'));
      console.log(clickBackground)
      if (clickBackground.hasAttribute('active') == false) {
        gsap.to(clickBackground, .2, {
          autoAlpha: 1,
          duration: 0.2,
        })
        gsap.to("img[active]", .2, {
          autoAlpha: 0,
          duration: 0.2,
          delay: 0.3,
        })
        document.getElementById('palette').setAttribute('aria-colour', clickBackground.getAttribute('data-colour'))
        document.querySelector('img[active]').removeAttribute('active');
        clickBackground.setAttribute('active', '');
      }
    });
  })

  // BTS

  var videoContainer = document.querySelector('.bts-video#full-video');
  var docElement = document.querySelector('html');

  document.querySelector('#bts .video-content').addEventListener('click', function () {
    gsap.to(videoContainer, {
          autoAlpha: 1,
          duration: 0.2
        })
        var run = setInterval(function () { videoDuration(videoContainer); }, 10);
        document.querySelector('#bts .short-video').pause();
        docElement.classList.add("no-scroll");
        videoContainer.querySelector('video').play();
  });

  document.querySelector('#bts .bts-video .video-close.desk').addEventListener('click', function () {
    gsap.to(videoContainer, {
          autoAlpha: 0,
          duration: 0.2,
          onComplete: function() {
            document.querySelector('#bts .video-play-pause').setAttribute('aria-play', 'true')
          }
        })
        clearTimeout(run);
        document.querySelector('#bts .short-video').play();
        docElement.classList.remove("no-scroll");
        videoContainer.querySelector('video').pause();
  });

  document.querySelector('#bts .bts-video .video-close.mob').addEventListener('click', function () {
    gsap.to(videoContainer, {
          autoAlpha: 0,
          duration: 0.2,
          onComplete: function() {
            document.querySelector('#bts .video-play-pause').setAttribute('aria-play', 'true')
          }
        })
        clearTimeout(run);
        document.querySelector('#bts .short-video').play();
        docElement.classList.remove("no-scroll");
        videoContainer.querySelector('video').pause();
  });


  // TIMELINE



  const images = gsap.timeline({
    scrollTrigger:{
      invalidateOnRefresh: true,
      trigger: "#lookbook",
      pin: true,
      start: "top top",
      end: "bottom -1000%",
      scrub: 1,
      ease: "none",
      onEnter: function() {
          console.log("ScrollTrigger initialized");
          
          var elms = document.getElementsByClassName( 'splide' );

          for ( var i = 0; i < elms.length; i++ ) {
            new Splide(elms[ i ] , {
              arrows: false,
              pagination: false,
              type   : 'loop',
              drag   : 'free',
              autoScroll: {
                pauseOnHover: false,
                speed: 0.5,
              },
            }).mount( window.splide.Extensions );
          }

        }
    }
  });

  images.to("#the-resort",{
    autoAlpha: 0,
    duration: 4,
    delay: 8
  }),
  images.to("#palette",{
    autoAlpha: 0,
    duration: 4,
    delay: 8,
  })
  images.to("#drop-1",{
    autoAlpha: 0,
    duration: 4,
    delay: 8,
  }),
  images.to("#drop-1-content",{
    autoAlpha: 1,
    duration: 4,
  }),
  images.to("#drop-1-content",{
    autoAlpha: 0,
    duration: 4,
    delay: 8,
  }),
  images.to("#drop-2",{
    autoAlpha: 1,
    duration: 4,
  }),
  images.to("#drop-2",{
    autoAlpha: 0,
    duration: 4,
    delay: 4,
  }),
  images.to("#drop-2-content",{
    autoAlpha: 1,
    duration: 10,
  }),
  images.to("#bts",{
    autoAlpha: 1,
    duration: 0,
    delay: 8,
  }),
  images.to("#bts #bts-video",{
    top: 0,
    duration: 12
  })

  var percent = (200 - ((window.innerWidth / document.querySelector("#bts .bts-content_container").offsetWidth ) * 100 ));
  console.log(percent);
  

  images.to("#bts .bts-content_container",{
    xPercent: - + percent,
    duration: 30
  })


  function videoDuration(video) {
    elemVideo = video.querySelector('video')
    video.querySelector('.duration-time').innerHTML = "- " + (elemVideo.duration - elemVideo.currentTime).toFixed(2);
  }
  
  
});


</script>

