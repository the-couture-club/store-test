<style type="text/css">
   .footer,
   .footer__bottom {
        background-color: {{ section.settings.footer_bg }};
    }
    {% if section.settings.footer_heading_color %}
        .footer-nav__title.footer-nav__title {
          color: {{ settings.color_headings }};
        }
    {% endif %}
</style>
<footer role="contentinfo" id="footer" class="footer footer--{{ section.settings.footer_style }}{% if section.settings.divider %} footer-divider{% endif %}">
    <div class="container">
        <div class="footer__content">
            <div class="o-layout">
                {%- liquid
                    case section.blocks.size
                        when 1
                            assign grid_item_width = 'u-1/1 u-1/2@desk'
                        when 2
                            assign grid_item_width = 'u-1/1 u-1/2@desk'
                        when 3
                            assign grid_item_width = 'u-1/1 u-1/3@desk'
                        when 4
                            assign grid_item_width = 'u-1/1 u-1/4@desk'
                    endcase
                -%}


                {% for block in section.blocks %}
                    {% if block.type == 'newsletter' %}
                    <div data-type="newsletter" class="o-layout__item {{ grid_item_width }}" {{ block.shopify_attributes }}>
                        <div class="footer-nav">
                            <h5 class="footer-nav__title cc-fs1">{{ 'general.newsletter_form.title' | t }}</h5>
                            <a class="cc-fs1">{{ 'general.newsletter_form.content' | t }}</a>
                            <div class="newsletter__form-wrapper">
                                <div class="klaviyo-form-VyawKe"></div>
                            </div>
                        </div>
                    </div>
                    {% endif %}
                    {% if block.type == 'linklist' %}
                        {% if linklists[block.settings.linklist].links.size > 0 %}
                            <div data-type="linklist" class="o-layout__item {{ grid_item_width }}" {{ block.shopify_attributes }}>
                                <div class="footer-nav">
                                    <accordion-group>
                                        <details id="footer-{{ forloop.index }}">
                                            <summary>
                                                <h6 class="footer-nav__title cc-fs1">{{ block.settings.title }}</h6>
                                                <summary-icon>
                                                    <i class="icon icon--plus" aria-hidden="true"></i>
                                                </summary-icon>
                                            </summary>

                                            <details-content>
                                                <div class="product-single__accordion__item-wrap rte">
                                                    <ul class="footer-nav__items o-list-bare">
                                                        {% for link in linklists[block.settings.linklist].links %}
                                                            <li class="footer-nav__item{% if link.active %} footer-nav__item--active{% endif %}"><a href="{{ link.url }}" class="footer-nav__link">{{ link.title }}</a></li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                                
                                            </details-content>
                                        </details>
                                    </accordion-group>
                                </div>
                            </div>
                        {% endif %}
                    {% endif %}
                    {% if block.type == 'contact' %}
                        <div class="o-layout__item {{ grid_item_width }}" {{ block.shopify_attributes }}>
                            <div class="footer-nav">
                                {% unless block.settings.title == blank %}
                                    <h3 class="footer-nav__title cc-fs1">{{ block.settings.title }}</h3>
                                {% endunless %}
                                {% if block.settings.show_email %}
                                    <p class="footer-nav__text"><a aria-label="Email" href="mailto:{{ shop.email }}" class="footer-nav__text-link">{{ shop.email }}</a></p>
                                {% endif %}
                                {% if block.settings.show_number %}
                                    <p class="footer-nav__text"><a aria-label="Call" href="tel:{{ shop.address.phone }}" class="footer-nav__text-link">{{ shop.address.phone }}</a></p>
                                {% endif %}
                                {% if block.settings.show_address %}
                                    <p class="footer-nav__text footer-nav__text--address">
                                        {{ shop.address.street }}<br/>
                                        {{ shop.address.city }} {{ shop.address.province }}<br/>
                                        {{ shop.address.zip }} {{ shop.address.country }}
                                    </p>
                                {% endif %}
                                {% if block.settings.show_social %}
