            "url": "",
            "liquid": "{% assign eventsAttended = 0 %}\n{% assign eventsUpcoming = 0 %}\n\n<div class=\"item-container\">\n    <div class=\"item-attended\">\n        {% for event in shop.metaobjects.clubhouse_events_list['clubhouse-events-list-jfm7bup7'].clubhouse_events_list.value %}\n            {% if customer.tags contains event['event_tag'] and event['event_completed'] == true %}\n                {% if eventsAttended == 0 %}\n                    <div class=\"item-title\">Events Attended</div>\n                {% endif %}\n                <div class=\"item-event\">\n                    <img src=\"https://cdn.shopify.com/s/files/1/0242/6047/6980/files/332765714-0303317f-6782-4511-9b29-90c78456d81b.png?v=1716384231\">\n                    <div class=\"item-event_content\">\n                        <span class=\"item-event_content--title\">{{ event['event_title'] }}</span>\n                        <div class=\"item-event_content--info\">\n                            <span class=\"material-symbols-outlined\">location_on</span>\n                            <span>{{ event['event_location'] }}</span>\n                        </div>\n                        <div class=\"item-event_content--info\">\n                            <span class=\"material-symbols-outlined\">schedule</span>\n                            <span>{{ event['event_date_time'] | date: \"%B %e at %l:%M %p %Z\" }}</span>\n                        </div>\n                    </div>\n                    <div class=\"item-event_content--cta\">Attended</div>\n                </div>\n                {% assign eventsAttended = eventsAttended | plus: 1 %}\n            {% endif %}\n        {% endfor %}\n    </div>\n    <div class=\"item-upcoming\">\n\n    {% for event in shop.metaobjects.clubhouse_events_list['clubhouse-events-list-jfm7bup7'].clubhouse_events_list.value %}\n        {% if event['event_completed'] == false %}\n            {% if eventsUpcoming == 0 %}\n                <div class=\"item-title\">Upcoming Events</div>\n            {% endif %}\n            <div class=\"item-event\">\n                <img src=\"https://cdn.shopify.com/s/files/1/0242/6047/6980/files/332765714-0303317f-6782-4511-9b29-90c78456d81b.png?v=1716384231\">\n                <div class=\"item-event_content\">\n                    <span class=\"item-event_content--title\">{{ event['event_title'] }}</span>\n                    <div class=\"item-event_content--info\">\n                        <span class=\"material-symbols-outlined\">location_on</span>\n                        <span>{{ event['event_location'] }}</span>\n                    </div>\n                    <div class=\"item-event_content--info\">\n                        <span class=\"material-symbols-outlined\">schedule</span>\n                        <span>{{ event['event_date_time'] | date: \"%B %e at %l:%M %p %Z\" }}</span>\n                    </div>\n                </div>\n              {% if event['event_signup_url'].value != null %} <a href=\"{{ event['event_signup_url'].value }}\" class=\"item-event_content--cta\">{{ event['cta_text'] }}</a> {% endif %}\n            </div>\n            {% assign eventsUpcoming = eventsUpcoming | plus: 1 %}\n        {% endif %}\n    {% endfor %}\n\n    {% if eventsUpcoming == 0 %}\n        <div>No Upcoming Events</div>\n    {%endif%}\n    </div>\n</div>"
