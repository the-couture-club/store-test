	<style>
		.cp-preloader {
			position: fixed;
			inset: 0;
			background: #fff;
			display: flex;
			align-items: center;
			justify-content: center;
			z-index: 9999;
			opacity: 1;
			visibility: visible;
			transition: opacity 0.4s ease, visibility 0.4s ease;
		}
		.cp-preloader.is-hidden {
			opacity: 0;
			visibility: hidden;
		}
		.cp-preloader__logo {
			width: 120px;
			height: 120px;
			object-fit: contain;
			animation: cp-pulse 2s ease-in-out infinite;
		}
		@keyframes cp-pulse {
			0% { transform: scale(0.96); opacity: 0.8; }
			25% { transform: scale(1.06); opacity: 1; }
			50% { transform: scale(0.96); opacity: 0.8; }
			75% { transform: scale(1.06); opacity: 1; }
			100% { transform: scale(0.96); opacity: 0.8; }
		}
	</style>
	<div class="cp-preloader" id="cp-preloader" aria-hidden="true">
		<img class="cp-preloader__logo" src="https://cdn.shopify.com/s/files/1/0242/6047/6980/files/TCC_ACTIVE_LOGO_BLACK_20_X_20-TRANSPARENT_BG.png?v=1764067965" alt="The Couture Club" loading="eager" />
	</div>
	<script>
		(function() {
			var preloader = document.getElementById('cp-preloader');
			if (!preloader) return;
			var hide = function() {
				if (!preloader) return;
				preloader.classList.add('is-hidden');
				setTimeout(function() {
					if (preloader && preloader.parentNode) preloader.parentNode.removeChild(preloader);
				}, 2000);
			};
			if (document.readyState === 'complete') {
				hide();
			} else {
				window.addEventListener('load', hide);
				setTimeout(hide, 2000);
			}
		})();
	</script>
